{
    "parser" : "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules":{
        "semi": "off",
        "arrow-parens":"off",
        "linebreak-style":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "no-fallthrough": "off"
    },
    "extends":["eslint:recommended", "google"]
}
